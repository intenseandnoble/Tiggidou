<div id="footer-wrapper">
    <footer id="footer" class="container">
        <div class="row">
            <div class="3u 6u(medium) 12u$(small)">

                <!-- Links -->
                <section class="widget links">
                    <h3><%= foot.colonne.num1.nom %></h3>
                    <ul class="style2">
                        <li><a href=<%= foot.colonne.num1.lien1.link %>><%= foot.colonne.num1.lien1.name %></a></li>

                    </ul>
                </section>

            </div>
            <div class="3u 6u$(medium) 12u$(small)">

                <!-- Links -->
                <section class="widget links">
                    <h3><%= foot.colonne.num2.nom %></h3>
                    <ul class="style2">
                        <li><a href=<%= foot.colonne.num2.lien1.link %>><%= foot.colonne.num2.lien1.name %></a></li>

                    </ul>
                </section>

            </div>
            <div class="3u 6u(medium) 12u$(small)">

                <!-- Links -->
                <section class="widget links">
                    <h3><%= foot.colonne.num3.nom %></h3>
                    <ul class="style2">
                        <li><a href=<%= foot.colonne.num3.lien1.link %>><%= foot.colonne.num3.lien1.name %></a></li>

                    </ul>
                </section>

            </div>
            <div class="3u 6u$(medium) 12u$(small)">

                <!-- Contact -->
                <section class="widget contact last">
                    <h3> <%= foot.contact %> </h3>
                    <ul>
                        <li><a href="#" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
                        <li><a href="#" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
                        <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
                        <li><a href="#" class="icon fa-dribbble"><span class="label">Dribbble</span></a></li>
                        <li><a href="#" class="icon fa-pinterest"><span class="label">Pinterest</span></a></li>
                    </ul>
                    <p><%= foot.adresse.rue %><br />
                        <%= foot.adresse.vpcp%><br />
                        <%= foot.adresse.numTel%></p>
                </section>

            </div>
        </div>
        <div class="row">
            <div class="12u">
                <div id="copyright">
                    <ul class="menu">
                        <li>&copy; Untitled. All rights reserved</li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
</div>