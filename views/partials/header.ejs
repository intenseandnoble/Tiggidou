<div id="header-wrapper">
    <header id="header" class="container">

        <!-- Logo -->
        <div id="logo">
            <h1><a href="/"> <%= header.nameCompany %> </a></h1>
            <span></span>
        </div>

        <!-- Nav -->
        <nav id="nav">
            <ul>
                <%if(!logged) {%>
                <li><a href="/sign-up"> <%= header.links.signup %></a></li>
                <% }%>
                <%if(!logged) {%>
                <li><a href="/login"> <%= header.links.login %></a></li>
                <% }%>
